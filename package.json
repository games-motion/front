{
  "name": "setup-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --ext src",
    "lint:code": "eslint --fix src",
    "format": "prettier-eslint --write \"src/**/*.{ts,tsx}\"",
    "test": "vitest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@radix-ui/react-tooltip": "^1.0.2",
